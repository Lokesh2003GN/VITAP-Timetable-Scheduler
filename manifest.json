{
  "manifest_version": 3,
  "name": "VITAP-Timetable-Scheduler",
  "version": "4.5.20",
  "description": "Enhancing the better way to use VTOP Timetable",
  
  "host_permissions": [
    "*://vtop.vit.ac.in/*",
    "*://vtopcc.vit.ac.in/vtop/*",
    "*://vtop.vitap.ac.in/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://vtop.vitap.ac.in/*",
        "*://vtopcc.vit.ac.in/vtop/*"
      ],
      "js": [
        "js/createTimeTable.js",
        "js/parsersTimeTable.js",
        "js/timetable.js",
        "js/schedule.js"
      ]
    }
  ],
  "icons": {
    "16": "assets/icons/img_16.png",
    "48": "assets/icons/img_48.png",
    "128": "assets/icons/img_128.png"
  }
}
